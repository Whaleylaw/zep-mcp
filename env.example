# Zep MCP Server Configuration
# Copy this file to .env and fill in your values

# Required: Your Zep Cloud API key (get one at https://app.getzep.com/)
ZEP_API_KEY=your_zep_api_key_here

# Multi-user configuration (comma-separated list)
ZEP_USER_IDS=aaron_whaley,tech_knowledge_base

# Default user ID (must be in ZEP_USER_IDS list)
ZEP_DEFAULT_USER_ID=aaron_whaley

# Optional: Server configuration
# HOST=0.0.0.0
# PORT=8052
# TRANSPORT=sse

# Optional: Logging configuration
# LOG_LEVEL=INFO 